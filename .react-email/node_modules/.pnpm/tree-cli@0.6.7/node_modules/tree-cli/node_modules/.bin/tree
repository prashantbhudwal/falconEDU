#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ankitbisht/Documents/main-falcon/falconone/.react-email/node_modules/.pnpm/tree-cli@0.6.7/node_modules/tree-cli/bin/node_modules:/Users/ankitbisht/Documents/main-falcon/falconone/.react-email/node_modules/.pnpm/tree-cli@0.6.7/node_modules/tree-cli/node_modules:/Users/ankitbisht/Documents/main-falcon/falconone/.react-email/node_modules/.pnpm/tree-cli@0.6.7/node_modules:/Users/ankitbisht/Documents/main-falcon/falconone/.react-email/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ankitbisht/Documents/main-falcon/falconone/.react-email/node_modules/.pnpm/tree-cli@0.6.7/node_modules/tree-cli/bin/node_modules:/Users/ankitbisht/Documents/main-falcon/falconone/.react-email/node_modules/.pnpm/tree-cli@0.6.7/node_modules/tree-cli/node_modules:/Users/ankitbisht/Documents/main-falcon/falconone/.react-email/node_modules/.pnpm/tree-cli@0.6.7/node_modules:/Users/ankitbisht/Documents/main-falcon/falconone/.react-email/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/tree" "$@"
else
  exec node  "$basedir/../../bin/tree" "$@"
fi
